{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add angular-custom-webpack-chaining",
      "factory": "./ng-add/index#ngAdd",
			"schema": "./ng-add/schema.json"
    },
		"ng-add-custom-webpack": {
			"description": "Add @angular-builders/custom-webpack",
			"factory": "./ng-add-custom-webpack/index#ngAddCustomWebpack",
			"schema": "./ng-add-custom-webpack/schema.json"
		},
		"add-chain": {
			"description": "Add webpack chain",
			"factory": "./add-chain/index#addChain",
			"schema": "./add-chain/schema.json"
		}
		
  }
}
